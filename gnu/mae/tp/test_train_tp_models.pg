reset
set terminal postscript eps enhanced color font 'Arial,21'
set output 'tp.eps'
set term post size 6,2

set datafile missing "-"
set key noenhanced
set title "Model Evaluation - Throughput"

#set xlabel "Time (sec)"
set ylabel "MAE (kbps)"

set auto x

set key outside horiz top right

set style data histogram 
set mytics 2
set style histogram cluster gap 1

set style fill solid

set boxwidth 0.9
set xtics format ""

set grid

set yrange [0:7]

set border lw 2
set xtics font ",15"
set ytics font ",15"

set key font ",12"

plot "test_train_tp_models.dat" using 2:xtic(1) title "Training" linecolor "#006400",\
	"test_train_tp_models.dat" using 3 title "Testing" linecolor "#5CDC64"
